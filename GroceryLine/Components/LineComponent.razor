@using GroceryLine.Models;

<div class="line">
    <div class="line-start">

    </div>

    @foreach(Customer customer in Line.Customers)
    {
        <div class="customer">
            <p class="item-count">@customer.ItemCount</p>
        </div>
    }
</div>

@code {
    [Parameter, EditorRequired]
    public Line Line { get; set; } = default!;
}
